@inject CommonLocalizationService localizer

<!DOCTYPE html>
<html lang="da-DA">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, minimum-scale=1.0" />
    <title>@ViewData["Title"]Echo</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Echo.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://use.typekit.net/jfw1snt.css">
    <link rel="stylesheet" href="~/lib/fortawesome/fontawesome-free/css/all.css" />
    
</head>
<body>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>